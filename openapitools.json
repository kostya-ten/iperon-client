{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.1",
    "generators": {
      "typescript": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/output/typescript/",
        "glob": "openapi.json",
        "additionalProperties": {
          "npmName": "iperon",
          "supportsES6": "true",
          "withInterfaces": true,
          "typescriptThreePlus": true
        }
      }
    }
  }
}
